package es.david.httptest;

public class Message {
    private String message;

    public Message setMessage(final String message) {
        this.message = message;
        return this;
    }

    public String getMessage() {
		return message;
	}

	@Override
    public String toString() {
        return "Message{" +
                "mesage='" + message + '\'' +
                '}';
    }
}
